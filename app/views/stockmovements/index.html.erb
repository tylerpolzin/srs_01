<p id="notice"><%= notice %></p>

<h1>Stockmovements</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stockmovements.each do |stockmovement| %>
      <tr>
        <td><%= link_to 'Show', stockmovement %></td>
        <td><%= link_to 'Edit', edit_stockmovement_path(stockmovement) %></td>
        <td><%= link_to 'Destroy', stockmovement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stockmovement', new_stockmovement_path %>
