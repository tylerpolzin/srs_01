<p id="notice"><%= notice %></p>

<h1>Storeorders</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @storeorders.each do |storeorder| %>
      <tr>
        <td><%= link_to 'Show', storeorder %></td>
        <td><%= link_to 'Edit', edit_storeorder_path(storeorder) %></td>
        <td><%= link_to 'Destroy', storeorder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Storeorder', new_storeorder_path %>
